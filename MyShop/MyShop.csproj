  </ItemGroup>

  <ItemGroup>
    <None Update="Images\KhachHang\avatar_01.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Images\KhachHang\avatar_02.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Images\KhachHang\avatar_03.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Images\KhachHang\avatar_04.jpg">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
